<% include ../partials/header %>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Firstname</th>
            <th>Lastname</th>
        </tr>
    </thead>
    <tbody>
        <% subjects.forEach(function(subject) { %>
        <tr>
            <td scope="rows"><%= subject.code %></td>
            <td><%= subject.title %></td>
            <td><%= subject.credits %></td>
            <td><%= subject.sections %></td>
        </tr>
        <% }); %>
        
        <tr>
            <td scope="rows"></td>
            <td></td>
            <td></td>
        </tr>
    </tbody>
</table>
