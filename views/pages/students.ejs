<% include ../partials/header %>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Firstname</th>
            <th>Lastname</th>
        </tr>
    </thead>
    <tbody>
        <% students.forEach(function(student) { %>
        <tr>
            <td scope="rows"><%= student.id %></td>
            <td><%= student.title %></td>
            <td><%= student.firstname %></td>
            <td><%= student.lastname %></td>
        </tr>
        <% }); %>
        
        <tr>
            <td scope="rows"></td>
            <td></td>
            <td></td>
        </tr>
    </tbody>
</table>





<% include ../partials/footer %>